
{
  "pyprland": {
    "plugins": [
      "scratchpads",
      "monitors",
      "workspaces_follow_focus"
    ]
  },
  "scratchpads": {
    "term": {
      "command": "kitty --class kitty-dropterm",
      "animation": "fromBottom",
      "unfocus": "hide",
      "position": "50% 20%",
      "size": "50% 80%"
    }
  },
  "monitors": {
    "placement": {
      "EPSON PJ": {
        "rightOf": "eDP-1"
      }
    },
    "unknown": "wlrlui"
  }
}
